    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "chalk": "4.1.1",
    "eslint": "7.28.0",
    "eslint-plugin-cypress": "2.11.3",
    "jest": "26.6.3",
    "prettier": "2.3.1",
    "ts-node": "10.0.0",
    "tslib": "2.2.0",
    "typescript": "4.3.2"
  },
  "workspaces": [
    "./packages/*"
  ],
  "jest": {
