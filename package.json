{
  "name": "aiacta",
  "private": true,
  "scripts": {
    "build": "yarn workspace @aiacta/distribution build",
    "test": "jest",
    "run-script": "cd scripts && ts-node"
  },
  "dependencies": {
    "@swc-node/jest": "1.4.3",
    "@swc-node/register": "1.4.2",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.26",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "chalk": "5.0.1",
    "eslint": "8.14.0",
    "eslint-plugin-cypress": "2.12.1",
    "jest": "27.5.1",
    "prettier": "2.6.2",
    "ts-node": "10.7.0",
    "tslib": "2.4.0",
    "typescript": "4.6.3",
    "zx": "6.1.0"
  },
  "workspaces": [
    "./packages/*"
  ],
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc-node/jest"
      ]
    },
    "transformIgnorePatterns": [],
    "testPathIgnorePatterns": [
      "<rootDir>/.*/cypress"
    ]
  },
  "packageManager": "yarn@3.1.0"
}
