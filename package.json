{
  "name": "aiacta",
  "private": true,
  "scripts": {
    "build": "yarn workspace @aiacta/distribution build",
    "test": "jest",
    "run-script": "cd scripts && ts-node"
  },
  "dependencies": {
    "@swc-node/jest": "1.4.1",
    "@swc-node/register": "1.4.0",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.10",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "chalk": "5.0.0",
    "eslint": "8.3.0",
    "eslint-plugin-cypress": "2.12.1",
    "jest": "27.3.1",
    "prettier": "2.5.0",
    "ts-node": "10.4.0",
    "tslib": "2.3.1",
    "typescript": "4.5.2",
    "zx": "4.2.0"
  },
  "workspaces": [
    "./packages/*"
  ],
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc-node/jest"
      ]
    },
    "transformIgnorePatterns": [],
    "testPathIgnorePatterns": [
      "<rootDir>/.*/cypress"
    ]
  },
  "packageManager": "yarn@3.1.0"
}
