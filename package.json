{
  "name": "aiacta",
  "private": true,
  "scripts": {
    "build": "yarn workspace @aiacta/distribution build",
    "test": "jest",
    "run-script": "cd scripts && ts-node"
  },
  "dependencies": {
    "@swc-node/jest": "1.1.0",
    "@swc-node/register": "1.0.4",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.35",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "chalk": "4.1.0",
    "eslint": "7.21.0",
    "eslint-plugin-cypress": "2.11.2",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "ts-node": "9.1.1",
    "tslib": "2.1.0",
    "typescript": "4.2.3"
  },
  "workspaces": [
    "./packages/*"
  ],
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc-node/jest"
      ]
    },
    "testPathIgnorePatterns": [
      "<rootDir>/.*/cypress"
    ]
  }
}
