{
  "name": "@aiacta/frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && yarn i18n:extract && yarn i18n:compile",
    "serve": "vite preview",
    "generate": "graphql-codegen && prettier --write src/api/hooks.ts",
    "i18n:extract": "formatjs extract 'src/**/*.ts*' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "i18n:compile": "formatjs compile lang/en.json --ast --out-file dist/lang/en.json"
  },
  "dependencies": {
    "@babylonjs/core": "4.2.0",
    "@babylonjs/gui": "4.2.0",
    "@babylonjs/inspector": "4.2.0",
    "@babylonjs/materials": "4.2.0",
    "@rsuite/icons": "1.0.0",
    "@urql/devtools": "2.0.3",
    "@urql/exchange-auth": "0.1.2",
    "@urql/exchange-graphcache": "4.0.0",
    "cannon": "0.6.2",
    "csx": "10.0.2",
    "draft-js": "0.11.7",
    "framer-motion": "4.1.3",
    "graphql": "15.5.0",
    "graphql-tag": "2.12.1",
    "graphql-ws": "4.4.2",
    "history": "5.0.0",
    "msw": "0.27.0",
    "react": "17.0.1",
    "react-babylonjs": "3.0.4",
    "react-dom": "17.0.1",
    "react-error-boundary": "3.1.0",
    "react-hook-form": "7.0.7",
    "react-icons": "4.2.0",
    "react-intl": "5.13.2",
    "react-router-dom": "6.0.0-beta.0",
    "recoil": "0.2.0",
    "rsuite": "5.0.0-alpha.4",
    "typestyle": "2.1.0",
    "urql": "2.0.2"
  },
  "devDependencies": {
    "@formatjs/cli": "4.2.10",
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@graphql-codegen/typescript-urql": "^2.0.6",
    "@graphql-codegen/urql-introspection": "1.2.0",
    "@types/draft-js": "^0",
    "@types/node": "14.14.35",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@vitejs/plugin-react-refresh": "1.3.1",
    "cypress": "^7.2.0",
    "prettier": "2.2.1",
    "typescript": "4.2.3",
    "vite": "2.0.5"
  }
}
