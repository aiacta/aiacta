{
  "name": "@aiacta/frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && yarn i18n:extract && yarn i18n:compile",
    "serve": "vite preview",
    "test": "yarn test:cypress",
    "test:jest": "jest",
    "test:cypress": "cypress run",
    "test:dev": "cypress open",
    "generate": "graphql-codegen && prettier --write src/api/hooks.ts",
    "i18n:extract": "formatjs extract 'src/**/*.ts*' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "i18n:compile": "formatjs compile lang/en.json --ast --out-file dist/lang/en.json"
  },
  "dependencies": {
    "@babel/runtime": "7.16.3",
    "@mantine/core": "3.2.3",
    "@mantine/hooks": "3.2.3",
    "@mantine/notifications": "3.2.3",
    "@mdx-js/runtime": "2.0.0-next.9",
    "@react-three/drei": "7.22.9",
    "@react-three/fiber": "7.0.20",
    "@urql/devtools": "2.0.3",
    "@urql/exchange-auth": "0.1.6",
    "@urql/exchange-graphcache": "4.3.5",
    "@urql/exchange-multipart-fetch": "0.1.13",
    "cannon-es": "0.18.0",
    "framer-motion": "5.3.3",
    "graphql": "16.0.1",
    "graphql-tag": "2.12.6",
    "graphql-ws": "5.5.5",
    "history": "5.1.0",
    "match-sorter": "6.3.1",
    "msw": "0.35.0",
    "playcanvas": "1.50.0",
    "react": "alpha",
    "react-dom": "alpha",
    "react-error-boundary": "3.1.4",
    "react-hook-form": "7.20.4",
    "react-icons": "4.3.1",
    "react-intl": "5.22.0",
    "react-jss": "10.8.2",
    "react-router-dom": "6.0.2",
    "recoil": "0.5.2",
    "remove": "0.1.5",
    "three": "0.135.0",
    "urql": "2.0.5"
  },
  "devDependencies": {
    "@formatjs/cli": "4.4.0",
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-operations": "2.2.1",
    "@graphql-codegen/typescript-urql": "3.4.1",
    "@graphql-codegen/urql-introspection": "2.1.0",
    "@swc-node/jest": "1.4.1",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.10",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/three": "0.132.2",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "cypress": "9.1.0",
    "jest": "27.3.1",
    "prettier": "2.5.0",
    "typescript": "4.5.2",
    "vite": "2.6.14"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc-node/jest"
      ]
    },
    "transformIgnorePatterns": [],
    "testPathIgnorePatterns": [
      "<rootDir>/cypress"
    ]
  }
}
