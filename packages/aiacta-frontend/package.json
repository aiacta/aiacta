{
  "name": "@aiacta/frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && yarn i18n:extract && yarn i18n:compile",
    "serve": "vite build && vite preview",
    "test": "yarn test:cypress",
    "test:jest": "jest",
    "test:cypress": "cypress run",
    "test:dev": "cypress open",
    "generate": "graphql-codegen && prettier --write src/api/hooks.ts",
    "i18n:extract": "formatjs extract 'src/**/*.ts*' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "i18n:compile": "formatjs compile lang/en.json --ast --out-file dist/lang/en.json"
  },
  "dependencies": {
    "@babel/runtime": "7.14.0",
    "@mantine/core": "1.2.1",
    "@mantine/hooks": "1.2.1",
    "@mantine/notifications": "1.2.1",
    "@mdx-js/runtime": "2.0.0-next.9",
    "@react-three/drei": "5.1.6",
    "@react-three/fiber": "6.1.5",
    "@urql/devtools": "2.0.3",
    "@urql/exchange-auth": "0.1.3",
    "@urql/exchange-graphcache": "4.1.1",
    "cannon-es": "0.17.1",
    "draft-js": "0.11.7",
    "framer-motion": "4.1.17",
    "graphql": "15.5.0",
    "graphql-tag": "2.12.4",
    "graphql-ws": "4.5.1",
    "history": "5.0.0",
    "msw": "0.28.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "3.1.3",
    "react-hook-form": "7.6.4",
    "react-icons": "4.2.0",
    "react-intl": "5.18.0",
    "react-jss": "10.6.0",
    "react-router-dom": "6.0.0-beta.0",
    "recoil": "0.3.1",
    "remove": "0.1.5",
    "three": "0.128.0",
    "urql": "2.0.3"
  },
  "devDependencies": {
    "@formatjs/cli": "4.2.15",
    "@graphql-codegen/cli": "1.21.4",
    "@graphql-codegen/typescript": "1.22.0",
    "@graphql-codegen/typescript-operations": "1.17.16",
    "@graphql-codegen/typescript-urql": "2.0.6",
    "@graphql-codegen/urql-introspection": "1.3.0",
    "@swc-node/jest": "1.3.0",
    "@types/draft-js": "0.11.3",
    "@types/jest": "26.0.23",
    "@types/node": "14.17.0",
    "@types/react": "17.0.6",
    "@types/react-dom": "17.0.5",
    "@types/three": "0.128.0",
    "@vitejs/plugin-react-refresh": "1.3.3",
    "cypress": "7.3.0",
    "jest": "26.6.3",
    "prettier": "2.3.0",
    "typescript": "4.2.4",
    "vite": "2.2.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc-node/jest"
      ]
    },
    "transformIgnorePatterns": [],
    "testPathIgnorePatterns": [
      "<rootDir>/cypress"
    ]
  }
}
