{
  "name": "@aiacta/frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && yarn i18n:extract && yarn i18n:compile",
    "serve": "vite preview",
    "test": "yarn test:cypress",
    "test:jest": "jest",
    "test:cypress": "cypress run",
    "test:dev": "cypress open",
    "generate": "graphql-codegen && prettier --write src/api/hooks.ts",
    "i18n:extract": "formatjs extract 'src/**/*.ts*' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "i18n:compile": "formatjs compile lang/en.json --ast --out-file dist/lang/en.json"
  },
  "dependencies": {
    "@babel/runtime": "7.17.9",
    "@mantine/core": "4.1.5",
    "@mantine/form": "4.1.5",
    "@mantine/hooks": "4.1.5",
    "@mantine/notifications": "4.1.5",
    "@mantine/spotlight": "4.1.5",
    "@mdx-js/runtime": "2.0.0-next.9",
    "@react-three/drei": "9.5.3",
    "@react-three/fiber": "8.0.12",
    "@urql/devtools": "2.0.3",
    "@urql/exchange-auth": "0.1.7",
    "@urql/exchange-graphcache": "4.3.6",
    "@urql/exchange-multipart-fetch": "0.1.14",
    "cannon-es": "0.19.0",
    "framer-motion": "6.3.1",
    "graphql": "16.3.0",
    "graphql-tag": "2.12.6",
    "graphql-ws": "5.7.0",
    "history": "5.3.0",
    "match-sorter": "6.3.1",
    "msw": "0.39.2",
    "playcanvas": "1.53.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-error-boundary": "3.1.4",
    "react-hook-form": "7.30.0",
    "react-icons": "4.3.1",
    "react-intl": "5.25.0",
    "react-jss": "10.9.0",
    "react-router-dom": "6.3.0",
    "recoil": "0.7.2",
    "remove": "0.1.5",
    "three": "0.139.2",
    "urql": "2.2.0"
  },
  "devDependencies": {
    "@formatjs/cli": "4.8.3",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript": "2.4.8",
    "@graphql-codegen/typescript-operations": "2.3.5",
    "@graphql-codegen/typescript-urql": "3.5.7",
    "@graphql-codegen/urql-introspection": "2.1.1",
    "@swc-node/jest": "1.4.3",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.26",
    "@types/react": "18.0.6",
    "@types/react-dom": "18.0.2",
    "@types/three": "0.139.0",
    "@vitejs/plugin-react": "1.3.1",
    "babel-plugin-formatjs": "10.3.19",
    "cypress": "9.5.4",
    "jest": "27.5.1",
    "prettier": "2.6.2",
    "typescript": "4.6.3",
    "vite": "2.9.5"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc-node/jest"
      ]
    },
    "transformIgnorePatterns": [],
    "testPathIgnorePatterns": [
      "<rootDir>/cypress"
    ]
  }
}
